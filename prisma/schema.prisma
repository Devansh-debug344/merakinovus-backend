generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model contact_messages {
  id         Int       @id @default(autoincrement())
  name       String
  email      String
  message    String
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
